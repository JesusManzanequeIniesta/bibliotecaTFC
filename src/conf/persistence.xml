<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="bibliotecajpaPU" transaction-type="RESOURCE_LOCAL">
      <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
      <non-jta-data-source>jdbc/Biblioteca</non-jta-data-source>
    <class>entidades.Estado</class>
    <class>entidades.Peticion</class>
    <class>entidades.Prestamo</class>
    <class>entidades.Libro</class>
    <class>entidades.GeneratorTable</class>
    <class>entidades.Socio</class>
    <class>entidades.Grupos</class>
    <class>entidades.Ejemplar</class>
    <class>entidades.Tiposancion</class>
    <class>entidades.Autor</class>
    <class>entidades.Usuarios</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
        <property name="eclipselink.cache.shared.default" value="false"/>
        <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
</persistence>
